{
  "description" : "Ingestion pipeline",
  "processors" : [
    {
      "azurestorage" : {
        "container_field" : "azurestorage.container",
        "blob_field" : "azurestorage.blob",
        "target_field": "doc.raw"
      }
    },
    {
      "attachment" : {
        "field" : "doc.raw",
        "target_field": "doc"
      }
    },
    {
      "textanalytics" : {
        "text_field" : "doc.content",
        "language_field" : "doc.language",
        "key_phrases_field": "textanalytics.keyphrases",
        "sentiment_field": "textanalytics.sentiment"
      }
    }
  ]
}
